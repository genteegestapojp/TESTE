{
  "version": 2,
  "buildCommand": "echo 'Build completed'",
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/api/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/api/js/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|svg|ico|json))",
      "dest": "/api/$1"
    }
  ]
}
